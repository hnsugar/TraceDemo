<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_trace_record"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
   >

    <com.itgowo.sport.trace.tracedemo.Other.TitleBarView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></com.itgowo.sport.trace.tracedemo.Other.TitleBarView>

    <com.itgowo.sport.trace.tracedemo.Other.XListView.XListView
        android:id="@+id/listView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/v180_color3"
        android:divider="@color/v180_color11"
        android:dividerHeight="0.5dp"
        ></com.itgowo.sport.trace.tracedemo.Other.XListView.XListView>
</LinearLayout>
